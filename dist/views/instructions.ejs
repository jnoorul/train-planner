                                            <span class="hljs-attribute">lines</span>: [<span class="hljs-string">'purple'</span>]
                            }, {

                                            <span class="hljs-attribute">name</span>: <span class="hljs-string">'Serangoon'</span>,
                                            <span class="hljs-attribute">passengers</span>: <span class="hljs-number">40</span>,
                                            <span class="hljs-attribute">connectedStations</span>: [<span class="hljs-string">'Sengkang'</span>, <span class="hljs-string">'DhobyGhaut'</span>, <span class="hljs-string">'PayaLebar'</span>, <span class="hljs-string">'Bishan'</span>],
                                            <span class="hljs-attribute">lines</span>: [<span class="hljs-string">'purple'</span>, <span class="hljs-string">'circle'</span>]
                            } {

                                            <span class="hljs-attribute">name</span>: <span class="hljs-string">'DhobyGhaut'</span>,
                                            <span class="hljs-attribute">passengers</span>: <span class="hljs-number">20</span>,
                                            <span class="hljs-attribute">connectedStations</span>: [<span class="hljs-string">'Serangoon'</span>, <span class="hljs-string">'HarbourFront'</span>, <span class="hljs-string">'MarinaBay'</span>, <span class="hljs-string">'Orchard'</span>, <span class="hljs-string">'PayaLebar'</span>],
                                            <span class="hljs-attribute">lines</span>: [<span class="hljs-string">'purple'</span>, <span class="hljs-string">'red'</span>, <span class="hljs-string">'circle'</span>]

                            }, {

